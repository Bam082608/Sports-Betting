[run]
source = .
omit =
    */venv/*
    */env/*
    */.venv/*
    setup.py
    */site-packages/*
    *_watchtower_analysis.py
    verification/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
precision = 2
